{
  "$schema": "http://json-schema.org/schema",
  "$id": "#ContactPostRequest",
  "type": "object",
  "description": "The contact request object for creation.",
  "properties": {
    "Contact": {
      "type": "object",
      "properties": {
        "City": {
          "type": "string"
        },
        "Comment": {
          "type": "string"
        },
        "Country": {
          "type": "string"
        },
        "Email": {
          "type": "string"
        },
        "Fax": {
          "type": "string"
        },
        "Firstname": {
          "type": "string"
        },
        "Lastname": {
          "type": "string"
        },
        "Login": {
          "type": "string"
        },
        "Mobile": {
          "type": "string"
        },
        "OrganisationIDs": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "Password": {
          "type": "string"
        },
        "Phone": {
          "type": "string"
        },
        "PrimaryOrganisationID": {
          "type": "integer"
        },
        "Street": {
          "type": "string"
        },
        "Title": {
          "type": "string"
        },
        "Zip": {
          "type": "string"
        },
        "ValidID": {
          "type": "integer"
        }
      },
      "required": [
        "Firstname",
        "Email",
        "Lastname",
        "Login",
        "OrganisationIDs",
        "PrimaryOrganisationID"
      ]
    }
  },
  "required": [
    "Contact"
  ]
}