{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#TextModulePatchRequest",
  "type": "object",
  "description": "The text module request object for update.",
  "properties": {
    "TextModule": {
      "type": "object",
      "properties": {
        "Comment": {
          "type": "string"
        },
        "Keywords": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },        
        "Language": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "Subject": {
          "type": "string"
        },
        "Text": {
          "type": "string"
        },
        "ValidID": {
          "type": "integer"
        }
      }
    }
  },
  "required": [
    "TextModule"
  ]
}