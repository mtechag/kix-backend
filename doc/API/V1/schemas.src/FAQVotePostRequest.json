{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#FAQVotePostRequest",
  "type": "object",
  "description": "The FAQ vote request object for create.",
  "properties": {
    "FAQVote": {
      "type": "object",
      "properties": {
        "CreatedBy": {
          "type": "integer"
        },
        "IPAddress": {
          "type": "string"
        },
        "Interface": {
          "type": "string",
          "description": "\"agent\" or \"customer\""
        },
        "Rating": {
          "type": "integer",
          "description": "an integer from 1 to 5"
        }
      },
      "required": [
        "Name"
      ]
    }
  },
  "required": [
    "IPAddress",
    "Interface",
    "Rating"
  ]
}