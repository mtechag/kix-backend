    "FAQCategory": {
      "type": "object",
      "properties": {
        "Name": {
          "type": "string"
        },
        "Comment": {
          "type": "string"
        },
        "ParentID": {
          "type": "integer"
        },
        "ValidID": {
          "type": "integer"
        }
      },
