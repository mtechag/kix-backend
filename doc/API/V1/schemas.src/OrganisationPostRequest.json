{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#OrganisationPostRequest",
  "type": "object",
  "description": "The organisation request object for creation.",
  "properties": {
    "Organisation": {
      "type": "object",
      "properties": {
        "City": {
          "type": "string"
        },
        "Comment": {
          "type": "string"
        },
        "Country": {
          "type": "string"
        },
        "Number": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "Street": {
          "type": "string"
        },
        "Url": {
          "type": "string"
        },
        "ValidID": {
          "type": "integer"
        },
        "Zip": {
          "type": "string"
        }
      },
      "required": [
        "Name",
        "Number"
      ]
    }
  },
  "required": [
    "Organisation"
  ]
}