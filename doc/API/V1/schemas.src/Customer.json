{
  "$schema": "http://json-schema.org/schema",
  "$id": "#Customer",
  "type": "object",
  "description": "The customer object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "City": {
      "type": "string",
      "readOnly": true
    },
    "Comment": {
      "type": "string",
      "readOnly": true
    },
    "Contacts": {
      "type": "array",
      "description": "added via include \"Contacts\"",
      "readOnly": true,
      "items": {
        "$ref": "Contact.json"
      }
    },
    "Country": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "ID": {
      "type": "integer",
      "readOnly": true
    },
    "Name": {
      "type": "string",
      "readOnly": true
    },
    "Street": {
      "type": "string",
      "readOnly": true
    },
    "Tickets": {
      "type": "array",
      "description": "added via include \"Tickets\"",
      "readOnly": true,
      "items": {
        "$ref": "Ticket.json"
      }
    },
    "URL": {
      "type": "string",
      "readOnly": true
    },
    "ZipCode": {
      "type": "string",
      "readOnly": true
    },
    "ValidID": {
      "type": "integer",
      "readOnly": true
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CustomerID",
    "CustomerIDs",
    "CreateBy",
    "CreateTime",
    "ID",
    "Name",
    "ValidID"
  ]
}