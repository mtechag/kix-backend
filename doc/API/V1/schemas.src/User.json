{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#User",
  "type": "object",
  "description": "The user object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "Preferences": {
      "type": "array",
      "description": "added via include \"Preferences\"",
      "item": {
        "$ref": "UserPreference.json"
      }
    },
    "RoleIDs": {
      "type": "array",
      "description": "added via include \"RoleIDs\"",
      "item": {
        "type": "integer"
      }
    },
    "UserComment": {
      "anyOf": [
        {"type": "string"},
        {"type": "null"}
      ],
      "readOnly": true
    },
    "UserEmail": {
      "type": "string",
      "readOnly": true
    },
    "UserFirstname": {
      "type": "string",
      "readOnly": true
    },
    "UserFullname": {
      "type": "string",
      "readOnly": true
    },
    "UserID": {
      "type": "integer",
      "readOnly": true
    },
    "UserLastname": {
      "type": "string",
      "readOnly": true
    },
    "UserLogin": {
      "type": "string",
      "readOnly": true
    },
    "UserMobile": {
      "anyOf": [
        {"type": "string"},
        {"type": "null"}
      ],
      "readOnly": true
    },
    "UserPhone": {
      "anyOf": [
        {"type": "string"},
        {"type": "null"}
      ],
      "readOnly": true
    },
    "UserTitle": {
      "type": "string",
      "readOnly": true
    },
    "ValidID": {
      "type": "integer",
      "readOnly": true
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CreateBy",
    "CreateTime",
    "UserID",
    "UserFirstname",
    "UserLastname",
    "UserLogin"
  ]
}