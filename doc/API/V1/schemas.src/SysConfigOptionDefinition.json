      "anyOf": [
        {"type": "object"},
        {"type": "null"}
      ],
      "readOnly": true
    },  
    "Type": {
      "type": "string",
      "readOnly": true
    },
    "ValidID": {
      "type": "integer",
      "readOnly": true
    },
    "Value": {
      "anyOf": [
        {"type": "object"},
        {"type": "string"},
        {"type": "null"}
      ],
      "readOnly": true
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CreateBy",
    "CreateTime",
    "Name",
    "Type",
    "ValidID"
  ]
}