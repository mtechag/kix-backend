{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#ServicePostRequest",
  "type": "object",
  "description": "The service request object for creation.",
  "properties": {
    "Service": {
      "type": "object",
      "properties": {
        "Comment": {
          "type": "string"
        },
        "Criticality": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "ParentID": {
          "type": "integer"
        },
        "TypeID": {
          "type": "integer"
        },
        "ValidID": {
          "type": "integer"
        }
      },
      "required": [
        "Criticality",
        "Name",
        "TypeID",
        "ValidID"
      ]
    }
  },
  "required": [
    "Service"
  ]
}