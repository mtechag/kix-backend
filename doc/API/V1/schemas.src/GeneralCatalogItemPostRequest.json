{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#GeneralCatalogItemPostRequest",
  "type": "object",
  "description": "The GeneralCatalog item request object for creation.",
  "properties": {
    "GeneralCatalogItem": {
      "type": "object",
      "properties": {
        "Class": {
          "type": "string"
        },        
        "Comment": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "ValidID": {
          "type": "integer"
        }
      },
      "required": [
        "Class",
        "Name"
      ]
    }
  },
  "required": [
    "GeneralCatalogItem"
  ]
}