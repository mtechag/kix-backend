{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#AuthRequest",
  "type": "object",
  "description": "An authorization object.",
  "properties": {
    "UserLogin": {
      "type": "string"
    },
    "UserType": {
      "type": "string",
      "enum": [
        "Agent",
        "Customer"
      ]
    },
    "Password": {
      "type": "string"
    }
  },
  "required": [
    "UserLogin",
    "UserType",
    "Password"
  ]
}