{
  "$schema": "http://json-schema.org/schema",
  "$id": "#JobTypeCollectionResponse",
  "type": "object",
  "description": "A list of job types.",
  "properties": {
    "MailAccountType": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
            "DisplayName": {
               "type": "string",
               "readOnly": true
            },
            "Name": {
              "type": "string",
              "readOnly": true
            }
        },
        "required": [
            "Name"
        ]  
      }
    }
  },
  "required": [
    "JobType"
  ]  
}