{
  "$schema": "http://json-schema.org/schema",
  "$id": "#Contact",
  "type": "object",
  "description": "The contact object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "City": {
      "type": "string",
      "readOnly": true
    },
    "Comment": {
      "type": "string",
      "readOnly": true
    },
    "Country": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "PrimaryOrganisationID": {
      "type": "integer",
      "readOnly": true
    },
    "OrganisationIDs": {
      "type": "array",
      "readOnly": true,
      "items": {
        "type": "integer"
      }
    },
    "Email": {
      "type": "string",
      "readOnly": true
    },
    "Fax": {
      "type": "string",
      "readOnly": true
    },
    "Firstname": {
      "type": "string",
      "readOnly": true
    },
    "ID": {
      "type": "integer",
      "readOnly": true
    },
    "Lastname": {
      "type": "string",
      "readOnly": true
    },
    "Login": {
      "type": "string",
      "readOnly": true
    },
    "Mobile": {
      "type": "string",
      "readOnly": true
    },
    "Phone": {
      "type": "string",
      "readOnly": true
    },
    "Street": {
      "type": "string",
      "readOnly": true
    },
    "Tickets": {
      "type": "array",
      "description": "added via include \"Tickets\"",
      "readOnly": true,
      "items": {
        "$ref": "Ticket.json"
      }
    },
    "Title": {
      "type": "string",
      "readOnly": true
    },
    "Zip": {
      "type": "string",
      "readOnly": true
    },
    "ValidID": {
      "type": "integer",
      "readOnly": true
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "PrimaryOrganisationID",
    "OrganisationIDs",
    "CreateBy",
    "CreateTime",
    "Firstname",
    "Email",
    "ID",
    "Lastname",
    "Login",
    "ValidID"
  ]
}