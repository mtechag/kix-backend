{
   "$schema": "http://json-schema.org/draft-07/schema",
   "$id": "#JobRun",
   "type": "object",
   "description": "The job run object.",
   "properties": {
      "CreateBy": {
         "type": "integer",
         "readOnly": true
      },
      "CreateTime": {
         "type": "string",
         "readOnly": true
      },
      "ID": {
         "type": "integer",
         "readOnly": true
      },
      "JobID": {
         "type": "integer",
         "readOnly": true
      },
      "MacroActionID": {
         "anyOf": [ 
            {
                "type": "integer"
            },
            {
                "type": "null"
            }
        ],
         "readOnly": true
      },
      "MacroID": {
        "anyOf": [ 
            {
                "type": "integer"
            },
            {
                "type": "null"
            }
        ],       
        "readOnly": true
      },
      "Message": {
        "type": "string",
        "readOnly": true
      },
      "ObjectID": {
        "anyOf": [ 
            {
                "type": "integer"
            },
            {
                "type": "null"
            }
        ],
        "readOnly": true
      },
      "Priority": {
        "type": "string",
        "readOnly": true
      },
      "RunID": {
        "type": "integer",
        "readOnly": true
      }
   },
   "required" : [
      "CreateBy",
      "CreateTime",
      "ID",
      "JobID",
      "Priority",
      "RunID"
   ]
}
