{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#FAQArticlePostRequest",
  "type": "object",
  "description": "The FAQ article request object for create.",
  "properties": {
    "FAQArticle": {
      "type": "object",
      "properties": {
        "Approved": {
          "type": "integer"
        },
        "ApprovalOff": {
          "type": "integer",
          "description": "if set to 1 approval is ignored"
        },
        "Attachments": {
          "type": "array",
          "items": {
            "$ref": "FAQAttachmentPostRequest.json"
          }
        },
        "CategoryID": {
          "type": "integer"
        },
        "ContentType": {
          "type": "string"
        },
        "CustomerVisible": {
          "type": "integer"
        },
        "Field1": {
          "type": "string",
          "description": "symptom"
        },
        "Field2": {
          "type": "string",
          "description": "problem"
        },
        "Field3": {
          "type": "string",
          "description": "solution"
        },
        "Field4": {
          "type": "string"
        },
        "Field5": {
          "type": "string"
        },
        "Field6": {
          "type": "string",
          "description": "comment"
        },
        "Keywords": {
          "type": "array",
          "item": {
            "type": "string"
          }
        },
        "Language": {
          "type": "string"
        },
        "Number": {
          "type": "string"
        },
        "Title": {
          "type": "string"
        },
        "ValidID": {
          "type": "integer"
        }
      },
      "required": [
        "CategoryID",
        "Title"
      ]
    }
  },
  "required": [
    "FAQArticle"
  ]
}