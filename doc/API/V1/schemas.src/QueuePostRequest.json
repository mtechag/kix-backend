{
  "$schema": "http://json-schema.org/schema",
  "$id": "#QueuePostRequest",
  "type": "object",
  "description": "The queue request object for creation.",
  "properties": {
    "Queue": {
      "type": "object",
      "properties": {
        "Calendar": {
          "type": "string"
        },
        "Comment": {
          "type": "string"
        },
        "DefaultSignKey": {
          "type": "integer"
        },
        "FirstResponseNotify": {
          "type": "integer"
        },
        "FirstResponseTime": {
          "type": "integer"
        },
        "FollowUpID": {
          "type": "integer"
        },
        "FollowUpLock": {
          "type": "integer"
        },
        "GroupID": {
          "type": "integer"
        },
        "Name": {
          "type": "string"
        },
        "ParentID": {
          "type": "integer"
        },
        "Signature": {
          "type": "string"
        },
        "SolutionNotify": {
          "type": "integer"
        },
        "SolutionTime": {
          "type": "integer"
        },
        "SystemAddressID": {
          "type": "integer"
        },
        "UnlockTimeout": {
          "type": "integer"
        },
        "UpdateNotify": {
          "type": "integer"
        },
        "UpdateTime": {
          "type": "integer"
        },
        "ValidID": {
          "type": "integer"
        }
      },
      "required": [
        "GroupID",
        "Name",
        "ValidID"
      ]
    }
  },
  "required": [
    "Queue"
  ]
}