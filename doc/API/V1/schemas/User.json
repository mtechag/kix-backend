{
  "$schema": "http://json-schema.org/schema",
  "$id": "#User",
  "type": "object",
  "description": "The user object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "UserEmail": {
      "type": "string"
    },
    "UserFirstname": {
      "type": "string"
    },
    "UserFullname": {
      "type": "string"
    },
    "UserID": {
      "type": "integer",
      "readOnly": true
    },
    "UserLastname": {
      "type": "string"
    },
    "UserLogin": {
      "type": "string"
    },
    "UserTitle": {
      "type": "string"
    },
    "ValidID": {
      "type": "integer"
    },
    "Preferences": {
      "type": "array",
      "item": {
        "$ref": "#UserPreference"
      }
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CreateBy",
    "CreateTime",
    "UserID",
    "UserFirstname",
    "UserLastname",
    "UserLogin"
  ]
}