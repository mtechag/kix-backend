{
   "$id" : "#FAQAttachmentPostRequest",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The FAQ article request object for create.",
   "type" : "object",
   "properties" : {
      "FAQAttachment" : {
         "type" : "object",
         "properties" : {
            "Content" : {
               "description" : "base64 encoded file ontent string",
               "type" : "string"
            },
            "ContentType" : {
               "type" : "integer"
            },
            "CustomerVisible" : {
               "type" : "integer"
            },
            "Filename" : {
               "type" : "string"
            },
            "Inline" : {
               "description" : " if attachment is an inline attachment of a text field property (e.g. \"Field1\") or 0 if not",
               "type" : "integer"
            }
         },
         "required" : [
            "Content",
            "ContentType",
            "Filename"
         ]
      }
   },
   "required" : [
      "FAQAttachment"
   ]
}
