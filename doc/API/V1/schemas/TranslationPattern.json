{
   "$id" : "#TranslationPattern",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The translation pattern object.",
   "type" : "object",
   "properties" : {
      "AvailableLanguages" : {
         "type" : "array",
         "item" : {
            "type" : "string"
         },
         "readOnly" : true
      },
      "ChangeBy" : {
         "type" : "integer",
         "readOnly" : true
      },
      "ChangeTime" : {
         "type" : "string",
         "readOnly" : true
      },
      "CreateBy" : {
         "type" : "integer",
         "readOnly" : true
      },
      "CreateTime" : {
         "type" : "string",
         "readOnly" : true
      },
      "ID" : {
         "type" : "integer",
         "readOnly" : true
      },
      "Languages" : {
         "description" : "added via include \"Languages\"",
         "type" : "array",
         "item" : {
            "$ref" : "TranslationLanguage.json"
         },
         "readOnly" : true
      },
      "Value" : {
         "type" : "string",
         "readOnly" : true
      }
   },
   "required" : [
      "ChangeBy",
      "ChangeTime",
      "CreateBy",
      "CreateTime",
      "ID",
      "Value"
   ]
}
