   "properties" : {
      "TranslationPattern" : {
         "type" : "object",
         "properties" : {
            "Languages" : {
               "type" : "array",
               "items" : {
                  "$ref" : "TranslationLanguage.json"
               }
            },
            "Value" : {
               "type" : "string"
            }
         },
         "required" : [
            "Value"
         ]
      }
   },
   "required" : [
      "TranslationPattern"
   ]
}
