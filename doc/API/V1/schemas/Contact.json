               "type" : "string"
            },
            {
               "type" : "null"
            }
         ],
         "readOnly" : true
      },
      "Country" : {
         "type" : "string",
         "readOnly" : true
      },
      "CreateBy" : {
         "type" : "integer",
         "readOnly" : true
      },
      "CreateTime" : {
         "type" : "string",
         "readOnly" : true
      },
      "Email" : {
         "type" : "string",
         "readOnly" : true
      },
      "Fax" : {
         "type" : "string",
         "readOnly" : true
      },
      "Firstname" : {
         "type" : "string",
         "readOnly" : true
      },
      "ID" : {
         "type" : "integer",
         "readOnly" : true
      },
      "Lastname" : {
         "type" : "string",
         "readOnly" : true
      },
      "Login" : {
         "type" : "string",
         "readOnly" : true
      },
      "Mobile" : {
         "type" : "string",
         "readOnly" : true
      },
      "OrganisationIDs" : {
         "type" : "array",
         "items" : {
            "type" : "integer"
         },
         "readOnly" : true
