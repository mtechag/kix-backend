         "type" : "integer",
         "readOnly" : true
      },
      "ChangeTime" : {
         "type" : "string",
         "readOnly" : true
      },
      "City" : {
         "type" : "string",
         "readOnly" : true
      },
      "Comment" : {
         "anyOf" : [
