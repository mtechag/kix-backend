      "Title" : {
         "type" : "string",
         "readOnly" : true
      },
      "ValidID" : {
         "type" : "integer",
         "readOnly" : true
      },
      "Zip" : {
         "type" : "string",
         "readOnly" : true
      }
   },
   "required" : [
      "ChangeBy",
      "ChangeTime",
      "PrimaryOrganisationID",
      "OrganisationIDs",
      "CreateBy",
      "CreateTime",
      "Firstname",
      "Email",
      "ID",
      "Lastname",
      "Login",
