{
  "$schema": "http://json-schema.org/schema",
  "$id": "#Translation",
  "type": "object",
  "description": "The translation object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "ID": {
      "type": "integer"
    },
    "Pattern": {
      "type": "string"
    },
    "Languages": {
      "type": "array",
      "item": {
        "$ref": "#TranslationLanguage"
      }
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CreateBy",
    "CreateTime",
    "ID",
    "Pattern"
  ]
}