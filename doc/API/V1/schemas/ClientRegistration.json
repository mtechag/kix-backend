{
  "$schema": "http://json-schema.org/schema",
  "$id": "#ClientRegistration",
  "type": "object",
  "description": "The client registration object.",
  "properties": {
    "ClientID": {
      "type": "string",
      "readOnly": true
    },
    "Authentication": {
      "type": "string",
      "readOnly": true
    },
    "CallbackURL": {
      "type": "string",
      "readOnly": true
    }
  },
  "required": [
    "ClientID",
    "CallbackURL"
  ]
}